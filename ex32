the_count = [1,2,3,4,5]
fruits = ['apple', 'orange', 'pears', 'banana']
change = [1, 'pennies', 2, 'dimes', 3, 'quarters']

for letter in the_count:
    print "this is count %d" % letter

for fruit in fruits:
    print "this fruit is called %s" % fruit

for i in change:
    print "i got %r" % i

elements = []

for i in change:
    if i == range(0,6):
        print "adding %d to the list." % i
        elements.append(i)

for i in elements:
    print "element was %d" % i

