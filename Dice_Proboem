import random
from collections import Counter
def simu(x):
    k = []
    for i in range(0,x):
        k.append(random.randint(1,6))
        i += 1
    p = []
    for i in range(1,7):
        p.append(Counter(k)[i])
        i += 1
    return max(p)
    
    
q =[]    
for i in range(0,100000):
    q.append(simu(10))
    i += 1
    
print Counter(q).most_common(10)
